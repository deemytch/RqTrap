.container
  .row
    .col-md-1
      %h4
        =link_to root_path do
          .glyphicon.glyphicon-home
    .col-md-11
      %h4= full_trap_name(@trap)
  .row
    .col-md-12
      %table.table.table-condensed.table-hover.table-listing#rqs-listing
        %tr
          %th
          %th time
          %th ip
          %th UA
          %th method
          %th url
          %th comment
          %th
        -@rqs.each do |rq|
          =render partial: 'request_line', locals: { rq: rq }
